<odoo>
	
        <template id="index">
            <title>Students</title>
            <t t-foreach="students" t-as="student">
              <p>
              		<b>Student: </b> <t t-esc="student"/>
              </p>
            </t>
        </template>
        
        
         <template id="mochoc123">
            <title>Mon Hoc</title>
            <t t-foreach="Mon Hoc" t-as="mh">
              <p>
              		<b>Tên môn học: </b> <t t-esc="mh.name"/>
              		<br/>
              		<t t-esc="mh.image"/>
              </p>
            </t>
        </template>
        

</odoo>