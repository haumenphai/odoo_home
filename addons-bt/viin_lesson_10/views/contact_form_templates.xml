<odoo>
    <data>
        <template id="viin_contactus_form" name="Contact Form (Opportunity)" inherit_id="website_crm.contactus_form"
                  customize_show="True">
            <xpath expr="//div[@class='form-group row form-field']" position="after">
                <div class="form-group row form-field">
                    <label class="col-lg-3 col-md-4 col-form-label" for="contact_name">Job Position</label>
                    <div class="col-lg-7 col-md-8">
                        <input type="text" class="form-control o_website_form_input" name="job_position"
                               t-att-value="request.params.get('function', '')"/>
                    </div>
                </div>
                <div class="form-group row form-field">
                    <label class="col-lg-3 col-md-4 col-form-label" for="contact_name">Website</label>
                    <div class="col-lg-7 col-md-8">
                        <input type="text" class="form-control o_website_form_input" name="website"
                               t-att-value="request.params.get('website', '')"/>
                    </div>
                </div>
                <div class="form-group row form-field">
                    <label class="col-lg-3 col-md-4 col-form-label" for="contact_name">Country2</label>
                    <div class="col-lg-7 col-md-8">
                        <select name="country_id" class="form-control">
                            <t t-foreach="request.env['res.country'].search([])" t-as="country">
                                <option t-att-value="country.id">
                                    <t t-esc="country.name"/>
                                </option>
                            </t>
                        </select>
                    </div>
                </div>
                <div class="form-group row form-field">
                    <label class="col-lg-3 col-md-4 col-form-label" for="contact_name">Company Size</label>
                    <div class="col-lg-7 col-md-8">
                        <input type="text" class="form-control o_website_form_input" name="company_size"
                               t-att-value="request.params.get('company_size', '')"/>
                    </div>
                </div>
            </xpath>
        </template>
    </data>
</odoo>
